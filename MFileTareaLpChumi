#enlazado
a.out: lista.o implementacion.o main.o
	gcc lista.o implementacion.o main.o
	
#compliacion de main.c
main.o: main.c lista.h implementacion.h
	gcc -c main.c

#compilacion de implementacion.c
implementacion.o: lista.h implementacion.c
	gcc -c implementacion.c

#compilacion de lista.o
lista.o: lista.c
	gcc -c lista.c
